{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": true
    },
    {
      "name": "Python: Jupyter Notebook",
      "type": "debugpy",
      "request": "launch",
      "module": "jupyter",
      "args": ["notebook"],
      "console": "integratedTerminal"
    }
  ]
}